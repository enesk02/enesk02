package girilen.say覺.pkg2.nin.kuvvetleri;
      // Girilen say覺ya kadar olan ikinin kuvvetleri?
import java.util.Scanner;

public class GirilenSay覺2NinKuvvetleri {

   public static void main(String[] args) {
     Scanner s= new Scanner(System.in);
     System.out.println("Say覺 giriniz :");
     int input = s.nextInt();
     int k= 1;
     while (k <= input){
         System.out.println( k );
         k = k*2;
     }
    }
    
}
